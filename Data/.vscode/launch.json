    "version": "0.2.0",
    "configurations": [

        {
            "name": "Monument",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/src/monument/build_dataset.py",
            "cwd": "${workspaceRoot}",
            "envFile": "${workspaceFolder}/env_files/.env",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Monument80",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/src/monument/build_dataset.py",
            "args": ["--nl", "raw_data/Monument/80/data.en", "--sparql", "raw_data/Monument/80/data.sparql", "--out_dir", "out_data/Monument/80"],
            "cwd": "${workspaceRoot}",
            "envFile": "${workspaceFolder}/env_files/.env",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Monument50",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/src/monument/build_dataset.py",
            "args": ["--nl", "raw_data/Monument/50/data.en", "--sparql", "raw_data/Monument/50/data.sparql", "--out_dir","out_data/Monument/50"],
            "cwd": "${workspaceRoot}",
            "envFile": "${workspaceFolder}/env_files/.env",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "LC-QuAD",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/src/lcquad/build_dataset.py",
